<div class="navbar navbar-fixed-top">
  <div class="navbar-inner">
    <h1>List#list</h1>
    <p>Find me in app/views/list/list.html.erb</p>
  </div>
</div>
<header style="padding-top: 100px;">
  
</header>
<div class="container">
<div class="row">
  <div class="span3">
    <ul class="nav nav-tabs nav-stacked artistlist" id="artists">
    <% for artist in @artists %>
      <li><a href="javascript:void(0);" id="artist-<%= artist.id %>"><i class="icon-list"></i><span><%= artist.artist_aliases[0].name %></a></span></li>
    <% end %>
    </ul>
  </div>
  <div class="span4" id="albumBlock">
    <% for artist in @artists %>
      <ul class="nav nav-tabs nav-stacked affix span4 albumlist" style="display: none; height: 80%; overflow: auto;" id="albums-<%= artist.id %>">
        <% for album in artist.albums %>
          <li><a href="javascript:void(0);" id="album-<%= album.id %>"><span>
            <% if album.name == nil %>
              Not In Album Tracks
            <% else %>
              <%= album.name %>
            <% end %>
          </span></a></li>
        <% end %>
      </ul>
    <% end %>
  </div>
  <div class="span5" id="trackBlock">
    <% for artist in @artists %>
      <% for album in artist.albums %>
        <ul class="nav nav-tabs nav-stacked affix span5 tracklist" style="display: none; height: 80%; overflow: auto;" id="tracks-<%= album.id %>">
          <% for track in album.tracks %>
            <li><a href="javascript:void(0);" id="track-<%= track.id %>"><span class="track_artist hide"><%= track.track_artist %></span><span class="track_name"><%= track.name %></span></a></li>
          <% end %>
        </ul>
      <% end %>
    <% end %>
  </div>
</div>
</div>
<div id="trackDetailModal" class="modal hide fade">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
    <h3 id="modalHeader">[<span id="trackDetailTitle"></span>]</h3>
  </div>
  <div class="modal-body">
    <div class="alert hide">
      Sorry. We cannot find this songs movie.
    </div>
    <div id="modalTrackDetail"></div>
  </div>
  <div class="modal-footer">
    this is footer
  </div>
</div>

